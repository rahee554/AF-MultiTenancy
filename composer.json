{
    "name": "artflow-studio/tenancy",
    "description": "A comprehensive multi-tenant Laravel package with admin dashboard and domain management. Optimized for performance with proper stancl/tenancy integration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artflow Studio",
            "email": "info@artflow-studio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "stancl/tenancy": "*",
        "predis/predis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}