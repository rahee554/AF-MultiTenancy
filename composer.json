{
    "name": "artflow-studio/tenancy",
    "description": "Enhanced Laravel multi-tenancy package built on top of stancl/tenancy with status management, homepage functionality, and Livewire compatibility.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artflow Studio",
            "email": "info@artflow-studio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "stancl/tenancy": "*",
        "livewire/livewire": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}