{
    "name": "artflow-studio/tenancy",
    "description": "High-performance multi-tenant Laravel package with optimized architecture, admin dashboard, and comprehensive tenant management",
    "version": "0.4.1",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "multi-tenant", "tenancy", "saas", "domain", "database", "artflow"],
    "authors": [
        {
            "name": "Artflow Studio",
            "email": "info@artflow-studio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "stancl/tenancy": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "homepage": "https://github.com/artflow-studio/tenancy",
    "support": {
        "issues": "https://github.com/artflow-studio/tenancy/issues",
        "source": "https://github.com/artflow-studio/tenancy"
    }
}
