{
    "name": "artflow-studio/tenancy",
    "description": "A comprehensive multi-tenant Laravel package with admin dashboard and domain management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artflow Studio",
            "email": "info@artflow-studio.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "stancl/tenancy": "^3.9",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtflowStudio\\Tenancy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
